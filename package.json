{
  "name": "ku4es-react-testing",
  "version": "1.0.0",
  "description": "kodmunkiâ„¢ Unitilities for ECMAScript React Testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/kodmunki/ku4es-react-testing"
  },
  "license": "MIT",
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "scripts": {
    "reload": "rm -rf ./node_modules && rm -f ./package-lock.json && npm install && npm run link",
    "test": "mocha -R spec --compilers js:babel-register --recursive ./test",
    "test-file": "mocha -R spec --compilers js:babel-register",
    "test-cov": "cross-env NODE_ENV=test nyc npm test",
    "test-cov-d": "open -a 'Google Chrome' ./coverage/lcov-report/index.html",
    "lint": "eslint './src/**/*.js' './test/**/*.js'",
    "lint-fix": "eslint --fix './src/**/*.js' './test/**/*.js'",
    "check": "npm run lint && npm run test-cov",
    "check-d": "npm run test-cov-d",
    "build": "shx mkdir -p dist && babel src -d dist --source-maps inline",
    "link": "sh ./link.sh"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "moxios": "0.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "axios": "0.x",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.x",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-3": "6.x",
    "cheerio": "1.0.0-rc.2",
    "cross-env": "^5.0.3",
    "eslint": "^4.7.2",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-react": "^7.4.0",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "react-addons-test-utils": "^15.6.2",
    "shx": "0.x",
    "teaspoon": "6.x"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist",
  "false": {}
}
