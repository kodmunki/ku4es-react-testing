{
  "name": "@kodmunki/ku4es-react-testing",
  "version": "1.3.0",
  "description": "kodmunkiâ„¢ Utilities for ECMAScript React Testing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodmunki/ku4es-react-testing.git"
  },
  "license": "MIT",
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "scripts": {
    "reload": "rm -rf ./node_modules && rm -f ./package-lock.json && npm install",
    "test": "mocha -R spec --require babel-register --recursive ./test",
    "test-file": "mocha -R spec --require js:babel-register",
    "test-cov": "cross-env NODE_ENV=test nyc npm test",
    "test-cov-d": "open -a 'Google Chrome' ./coverage/lcov-report/index.html",
    "lint": "eslint './src/**/*.js' './test/**/*.js'",
    "lint-fix": "eslint --fix './src/**/*.js' './test/**/*.js'",
    "check": "npm run lint && npm run test-cov",
    "check-d": "npm run test-cov-d",
    "docs": "npm run jsdocs && npm run mddocs",
    "docs-read": "open -a 'Google Chrome' ./jsdoc/index.html",
    "jsdocs": "rm -rf jsdoc ; jsdoc -c ./jsdoc.json",
    "mddocs": "rm README.API.md ; jsdoc2md --files 'src/**/*.js' > README.API.md",
    "build": "shx mkdir -p dist && babel src -d dist --source-maps inline",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@kodmunki/ku4es-kernel": "^1.0.1",
    "@kodmunki/ku4es-ui-testing": "^1.0.1",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "cheerio": "1.0.0-rc.2",
    "jsdom": "^11.5.1",
    "moxios": "^0.4.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "teaspoon": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "jsdoc-to-markdown": "^4.0.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.0",
    "shx": "^0.2.2"
  },
  "files": [
    "dist"
  ],
  "main": "dist",
  "bugs": {
    "url": "https://github.com/kodmunki/ku4es-react-testing/issues"
  },
  "homepage": "https://github.com/kodmunki/ku4es-react-testing#readme",
  "author": "kodmunki"
}
